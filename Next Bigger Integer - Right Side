#include <stdio.h>
int main()
{
int N;
scanf("%d",&N);
int arr[N];
for(int i=0;i<N;i++)
{
    scanf("%d",&arr[i]);
}
for(int i=0;i<N;i++)
{
    int ele=0,min=100001,f=0;
    for(int j=i+1;j<N;j++)
    {
        if(arr[i]<arr[j])
        {
            if(arr[j]-arr[i]<min)
            {
                min=arr[j]-arr[i];
                ele=arr[j];
                f=1;
            }
        }
    }
    if(f==1)
    printf("%d ",ele);
    else
    printf("%d ",arr[i]);
}
}
